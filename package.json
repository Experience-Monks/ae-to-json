{
  "name": "ae-to-json",
  "version": "1.0.0",
  "description": "will export an After Effects project as a JSON object",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Mikko Haapoja",
    "email": "me@mikkoh.com",
    "url": "https://github.com/jam3"
  },
  "dependencies": {
    "JSON2": "^0.1.0"
  },
  "devDependencies": {
    "after-effects": "^0.3.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "opn": "^4.0.1",
    "tape": "^4.5.1"
  },
  "scripts": {
    "test": "node test/",
    "prePublish": "npm run build-jsx",
    "build-jsx": "rm bundle.jsx; npm run copy-shim && npm run copy-get && npm run js-jsx",
    "copy-shim": "cat node_modules/after-effects/lib/includes/es5-shim.jsx >> bundle.jsx",
    "copy-get": "cat node_modules/after-effects/lib/includes/get.jsx >> bundle.jsx",
    "js-jsx": "browserify index.jsx >> bundle.jsx; babel bundle.jsx >> bundle.jsx"
  },
  "keywords": [
    "after,effects,json,jsx"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jam3/ae-to-json.git"
  },
  "homepage": "https://github.com/jam3/ae-to-json",
  "bugs": {
    "url": "https://github.com/jam3/ae-to-json/issues"
  }
}
