{
  "name": "ae-to-json",
  "version": "1.0.0",
  "description": "will export an After Effects project as a JSON object",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Mikko Haapoja",
    "email": "me@mikkoh.com",
    "url": "https://github.com/jam3"
  },
  "dependencies": {
    "JSON2": "^0.1.0"
  },
  "devDependencies": {
    "after-effects": "^0.3.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "npm-watch": "^0.1.1",
    "opn-cli": "^3.1.0",
    "tape": "^4.5.1"
  },
  "watch": {
    "test-build-run": {
      "patterns": ["src", "test"],
      "extensions": "js",
      "ignore": "test/aeToJSON.js"
    }
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run open-ae; npm-watch",
    "test": "npm run open-ae; npm run test-build-run;",
    "test-build-run": "npm run build-test; babel-node test/",
    "open-ae": "opn test/ae/testProject.aepx",
    "build-test": "npm run transpile; npm run test-template; npm run transpile-clean;",
    "test-template": "npm run test-cat-header; npm run test-browserify; npm run test-cat-footer",
    "test-cat-header": "cat test/testTemplate/header.js > test/aeToJSON.js",
    "test-browserify": "browserify index.js -s aeToJSON >> test/aeToJSON.js",
    "test-cat-footer": "cat test/testTemplate/footer.js >> test/aeToJSON.js",
    "transpile": "babel -d ./ src/",
    "transpile-clean": "rm *.js *.jsx; rm -rf util/",
    "copy-shim": "cat node_modules/after-effects/lib/includes/es5-shim.jsx > bundle.jsx",
    "copy-get": "cat node_modules/after-effects/lib/includes/get.jsx > bundle.jsx"
  },
  "keywords": [
    "after,effects,json,jsx"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jam3/ae-to-json.git"
  },
  "homepage": "https://github.com/jam3/ae-to-json",
  "bugs": {
    "url": "https://github.com/jam3/ae-to-json/issues"
  }
}
